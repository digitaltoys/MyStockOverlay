{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-appmy-stock-overlay",
  "version": "0.1.0",
  "identifier": "com.user.mystockoverlay",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "MyStockOverlay",
        "label": "main",
        "width": 800,
        "height": 600,
        "decorations": true,
        "transparent": false,
        "alwaysOnTop": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "window": {
      "all": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}